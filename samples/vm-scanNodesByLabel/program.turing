.data:
ENTITY_IDS $nodeIDs, {}
STRING     $person,  "Person"
OUTPUT     $out
LABELSET   $labelset

.alias:
ALIAS %out,      r0
ALIAS %labelset, r1
ALIAS %nodeIDs,  r2
ALIAS %label1,   r3

.text:
LOAD %out,      $out
LOAD %nodeIDs,  $nodeIDs
LOAD %labelset, $labelset

GET_LABEL %label1,   $person
ADD_LABEL %labelset, %label1

ADD_COL %out, %nodeIDs, 0

SCAN_NODES_BY_LABEL i0, %labelset, {%nodeIDs}
    i0_START:
    JEND   i0, i0_END
    UPDATE i0

    WRITE %out

    GOTO i0_START
i0_END:

EXIT
