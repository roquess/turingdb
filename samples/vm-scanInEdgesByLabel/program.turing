.data:
ENTITY_IDS $srcIDs,  {}
ENTITY_IDS $edgeIDs, {}
ENTITY_IDS $tgtIDs,  {}
STRING     $person,   "Person"
OUTPUT     $out
LABELSET   $labelset

.alias:
ALIAS %out,      r0
ALIAS %labelset, r1
ALIAS %srcIDs,   r2
ALIAS %edgeIDs,  r3
ALIAS %tgtIDs,   r4
ALIAS %types,    r5
ALIAS %label1,   r6

.text:
LOAD %out,      $out
LOAD %labelset, $labelset
LOAD %srcIDs,   $srcIDs
LOAD %edgeIDs,  $edgeIDs
LOAD %tgtIDs,   $tgtIDs

ALLOC_EDGE_TYPES %types

GET_LABEL %label1,   $person
ADD_LABEL %labelset, %label1

ADD_COL %out, %srcIDs,  0
ADD_COL %out, %edgeIDs, 0
ADD_COL %out, %types,   0
ADD_COL %out, %tgtIDs,  0

SCAN_IN_EDGES_BY_LABEL i0, %labelset, {%srcIDs, %edgeIDs, %tgtIDs, %types}
    i0_START:
    JEND   i0, i0_END
    UPDATE i0

    WRITE %out

    GOTO i0_START
i0_END:

DEALLOC_EDGE_TYPES %types

EXIT
